{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/telabase/**", 
        "destination": "/index.html"  
      },
      {
        "source": "**", 
        "destination": "/index.html"  
      }
    ]
  }
}
